{
  "name": "@hortau/ember-cli-graphql-file",
  "version": "1.0.0",
  "description": "An addon to precompile your `.graphql` files with `graphql-tag` and turn them into importable modules.",
  "keywords": [
    "ember-addon",
    "graphql",
    "graphql-tag"
  ],
  "repository": "https://github.com/Hortau/ember-cli-graphql-file",
  "license": "BSD-3-Clause",
  "author": "Charles Demers <cdemers@mirego.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "eslint ./*.js",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "7.8.0",
    "graphql-tag": "2.10.1"
  },
  "devDependencies": {
    "@ember/optional-features": "0.7.0",
    "babel-eslint": "10.0.2",
    "broccoli-asset-rev": "3.0.0",
    "broccoli-persistent-filter": "2.3.1",
    "ember-cli": "3.11.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-eslint": "5.1.0",
    "ember-cli-htmlbars": "3.1.0",
    "ember-cli-htmlbars-inline-precompile": "2.1.0",
    "ember-cli-uglify": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-load-initializers": "2.0.0",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "4.4.1",
    "ember-resolver": "5.1.3",
    "ember-source": "3.11.1",
    "ember-source-channel-url": "2.0.1",
    "ember-try": "1.2.1",
    "eslint": "6.0.1",
    "eslint-plugin-ember": "6.7.1",
    "eslint-plugin-node": "9.1.0",
    "graphql": "14.4.2",
    "loader.js": "4.7.0",
    "qunit-dom": "0.9.0"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "ember-cli-babel"
  }
}
